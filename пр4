# Задание 2
number = float(input("Введите число: "))

if number % 2 == 0:
    result = number ** 2
    print(f"Число кратно 2. {number}? = {result}")
elif number % 3 == 0:
    result = number ** 3
    print(f"Число кратно 3. {number}? = {result}")
else:
    result = number * 100
    print(f"Число {number} ? 100 = {result}")

# Задание 3
num1 = float(input("Введите первое число: "))
num2 = float(input("Введите второе число: "))

if num1 < 0 and num2 < 0:
    print(False)
elif num1 < 0:
    result1 = num1 + 1000
    print(f"Первое число отрицательное: {num1} + 1000 = {result1}")
    print(f"Второе число: {num2}")
elif num2 < 0:
    result2 = num2 + 1000
    print(f"Первое число: {num1}")
    print(f"Второе число отрицательное: {num2} + 1000 = {result2}")
else:
    print(True)
