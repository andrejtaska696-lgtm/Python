#3
r = int(input("Введите значение г: ") )
g = int(input("Введите значение д: ") )
b = int(input ("Введите значение b: ") )
if r == 0 and g== 0 and b== 0:
    print ( "Чёрный цвет")
elif r == 255 and g == 255 and b == 255:
    print ( "Белый цвет")
elif r == 255 and g == 0 and b == 0:
    print ( "Красный цвет")
elif g == 255 and r == 0 and b == 0: print ( "Зелёный цвет")
elif b == 255 and r == 0 and g == 0: 
    print (" Синий цвет")
else:
    print( "Нет цвета")
