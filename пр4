# Задание 2
a = float(input())
b = float(input())
c = float(input())
if a > 0 and b > 0 and c > 0 and (a + b > c) and (a + c > b) and (b + c > a):
    print(True)
else:
    print(False)

# Задание 3
n = int(input())
last_digit = abs(n) % 10
if last_digit == 0:
    result = n ** 10
elif last_digit == 1:
    result = n % 3
elif last_digit == 2:
    result = n // 2
else:
    result = n ** 2
print(result)
